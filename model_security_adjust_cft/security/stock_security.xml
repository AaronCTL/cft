<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>

<record model="ir.rule" id="stock_picking_restrict_rule">
        <field name="name">Stocking Picking Restrict Create/Update/Delete</field>
        <field name="model_id" search="[('model','=','stock.picking')]" model="ir.model"/>
        <field name="domain_force">[(1,'=',1)]</field>
        <field eval="[(4,ref('stock.group_stock_user'))]" name="groups"/>
    <field name="perm_unlink" eval="False"/>
    <field name="perm_write" eval="False"/>
    <field name="perm_create" eval="False"/>
</record>

<record model="ir.rule" id="stock_move_restrict_rule">
    <field name="name">Stock_Move Restrict Create/Update/Delete</field>
    <field name="model_id" search="[('model','=','stock.move')]" model="ir.model"/>
    <field name="domain_force">[(1,'=',1)]</field>
    <field eval="[(4,ref('stock.group_stock_user'))]" name="groups"/>
    <field name="perm_unlink" eval="False"/>
    <field name="perm_write" eval="False"/>
    <field name="perm_create" eval="False"/>
</record>

</data>
</openerp>
